{
    "trainer_class":"BaselineGDCTrainer",
    "lm_name": "mkhalifa/gpt2-biographies",
    "ref_lm_name": "mkhalifa/gpt2-biographies",
    "tk_name": "gpt2",
    "scorers":[
        {"name": "science", "config":{"scorer_type": "wikibio-wordlist", "scorer_attribute": "science"}},
